    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "1.0.0-beta.8",
    "@stencil/dev-server": "0.0.18",
    "@stencil/utils": "^0.0.5",
    "@types/jest": "^24.0.13",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxigenao/ox-CalendarPicker"
  },
  "author": "Pablo Ortu√±o",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oxigenao/ox-CalendarPicker"
  },
  "homepage": "https://github.com/oxigenao/ox-CalendarPicker",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
